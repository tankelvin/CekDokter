<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@color/bgcolor">

    <ImageButton
        android:id="@+id/btnProfile_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="42dp"
        android:background="@drawable/ic_hambuergerblack_home"
        />

    <ImageButton
        android:id="@+id/btnSearch_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="42dp"
        android:background="@drawable/ic_search_chat"
        />


    <!--Help 1 Layout-->
    <TextView
        android:id="@+id/txtJudul1_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="114dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:text="Pertanyaan yang sering diajukan"
        android:textSize="14dp"
        android:fontFamily="@font/psemibold"
        android:textColor="#9C9B9E"
        />

    <RelativeLayout
        android:id="@+id/relativelayout2_help"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtJudul1_help"
        android:layout_marginTop="12dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="32dp"
        android:background="@drawable/bg_help1_help">

        <TextView
            android:id="@+id/txtHelpAtas1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:text="Tidak bisa login"
            android:textSize="12dp"
            android:textColor="#FDBF50"
            android:fontFamily="@font/pmedium"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txtHelpAtas1"
            android:layout_marginTop="6dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:text="Apakah akun kamu sudah terhubung \ndengan Google atau Facebook ?"
            android:textSize="10dp"
            android:fontFamily="@font/pmedium"
            />
    </RelativeLayout>
    <!--Tutup Help 1 Layout-->

    <!--Help 2 Layout-->
    <TextView
        android:id="@+id/txtJudul2_help"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
