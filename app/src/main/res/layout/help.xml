        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:text="Masalah pembayaran"
        android:textSize="14dp"
        android:fontFamily="@font/psemibold"
        android:textColor="#9C9B9E"
        />

    <RelativeLayout
        android:id="@+id/relativelayout3_help"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/txtJudul2_help"
        android:layout_marginTop="12dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="32dp"
        android:background="@drawable/bg_help1_help">

        <TextView
            android:id="@+id/txtHelpTengah1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:text="Tidak bisa bayar \ndengan dompet gerak"
            android:textSize="12dp"
            android:textColor="#FDBF50"
            android:fontFamily="@font/pmedium"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txtHelpTengah1"
            android:layout_marginTop="6dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="20dp"
            android:layout_marginBottom="20dp"
            android:text="Dompet Gerak tidak bisa melakukan pembayaran dengan baik dan benar."
            android:textSize="10dp"
            android:fontFamily="@font/pmedium"
            />
    </RelativeLayout>
